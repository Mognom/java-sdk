{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Refund",
  "description": "Send a request of refund to Sipay",
  "type": "object",
  "properties": {
    "order": {
      "type": "string",
      "pattern": "^[\\w-]{6,64}$"
    },
    "reconciliation": {
      "type": "string",
      "pattern": "^[0-9]{4}[a-zA-Z0-9]{0,8}$"
    },
    "custom_01": {
      "type": "string"
    },
    "custom_02": {
      "type": "string"
    },
    "token": {
      "type": "string",
      "pattern": "^[\\w-]{6,128}$"
    },
    "transaction_id": {
      "type": "string",
      "pattern": "^[0-9]{6,22}$"
    }
  }
}